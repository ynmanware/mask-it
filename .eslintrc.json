{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "amd": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
          "error", "single"
        ]
    }
}
